{
	"name": "@portalpap/template-react-component-library",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/portalpap"
	},
	"version": "0.5.4",
	"description": "A simple template for a custom React component library",
	"scripts": {
		"build": "yarn remove:dist && yarn rollup",
		"remove:dist": "rmdir /s /q \"./dist/\"",
		"rollup": "rollup -c --context=window --environment BUILD:production"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/portalpap/template-react-component-library.git"
	},
	"author": "Troomi Wireless",
	"license": "ISC",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"devDependencies": {
		"@mdx-js/react": "^2.1.2",
		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-node-resolve": "^13.0.6",
		"@rollup/plugin-typescript": "^11.0.0",
		"@testing-library/react": "^13.3.0",
		"@types/lodash": "^4.14.168",
		"@types/lodash.clone": "^4.5.6",
		"@types/lodash.clonedeep": "^4.5.6",
		"@types/lodash.debounce": "^4.0.6",
		"@types/react": "^18.0.15",
		"@types/react-select": "4.0.18",
		"axios": "^0.19.2",
		"classnames": "^2.3.1",
		"identity-obj-proxy": "^3.0.0",
		"lodash.clone": "^4.5.0",
		"lodash.clonedeep": "^4.5.0",
		"lodash.debounce": "^4.0.8",
		"postcss": "^8.4.30",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.7.1",
		"react-select": "4.3.1",
		"react-toastify": "7.0.4",
		"resize-observer-polyfill": "^1.5.1",
		"rollup": "^2.60.0",
		"rollup-plugin-dts": "^4.0.1",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-postcss": "^4.0.1",
		"rollup-plugin-terser": "^7.0.2",
		"sass": "^1.43.5",
		"tslib": "^2.6.2",
		"typescript": "^4.7.4"
	},
	"peerDependencies": {
		"@types/react": ">=16.14.8",
		"react": ">=16.14.0",
		"react-dom": ">=16.14.0"
	},
	"peerDependenciesMeta": {
		"@types/react": {
			"optional": true
		}
	},
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"files": [
		"dist"
	],
	"types": "dist/index.d.ts",
	"packageManager": "yarn@1.22.19"
}
